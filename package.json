{
  "name": "xui-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lib-m": "vue-cli-service build --target lib --name xui-m --dest xui-m-ui/lib packages-mobile/index.js",
    "lib-pc": "vue-cli-service build --target lib --name xui --dest xui-ui/lib packages-pc/index.js",
    "lib-all": "npm run lib-pc && npm run lib-m"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "element": "^0.1.4",
    "element-ui": "^2.13.2",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-router": "^4.4.6",
    "@vue/cli-plugin-vuex": "^4.4.6",
    "@vue/cli-service": "^5.0.8",
    "copy-webpack-plugin": "^6.0.3",
    "less": "^3.12.2",
    "less-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.11",
    "xui-ui": "file:xui-ui"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
